source ~/.bin/git-completion.sh

alias g=git
alias gc="git commit -v"
alias gca="git commit --amend -v"
alias gs="git status"
alias gd="git diff"
alias gds="git diff --stat"
alias gdc="git diff --cached"
alias ga="git add"
alias gf="git fetch"
alias gb="git branch"
alias gg="git grep -En"

export RUBYOPT='rubygems'
